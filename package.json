{
  "name": "template",
  "description": "A template ",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "serve": "pnpm run serve:src",
    "serve:src": "vite --open",
    "serve:dist:dev": "pnpx browser-sync ./dist --single --no-notify --no-ui",
    "build": "pnpm run build:dev",
    "build:dev": "vite build --base='./' --sourcemap",
    "build:prod": "vite build --base='/portefolio/'",
    "version": "bash scripts/version.sh"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "esbuild": "^0.20.2",
    "vue": "^3.2.26",
    "vue-router": "^4.0.0-beta.13"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@vue/compiler-sfc": "npm:@vue/compiler-sfc@^3",
    "browser-sync": "^3.0.2",
    "sass": "^1.72.0",
    "tsconfig-paths": "^4.2.0",
    "vite": "^5.1.6"
  }
}
